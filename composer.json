{
  "name": "seablast/i18n",
  "description": "A lightweight internationalization (i18n) module designed for apps using the Seablast for PHP framework",
  "type": "library",
  "require": {
    "php": "^7.2 || ^8.0",
    "nette/utils": "^3.2.10 || ^4.0.5",
    "robmorgan/phinx": "^0.12.13 || ^0.13.4 || ^0.14.0 || ^0.15.5 || ^0.16.5",
    "seablast/interfaces": "^0.1",
    "tracy/tracy": "^2.9.8 || ^2.10.9",
    "webmozart/assert": "^1.10.0"
  },
  "require-dev": {
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-webmozart-assert": "^2.0",
    "phpunit/phpunit": "^8.4 || ^9 || ^10 || ^11 || ^12",
    "seablast/seablast": "^0.2.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stanislav Rejthar",
      "email": "rejthar@stanislavrejthar.com"
    }
  ],
  "support": {
    "email": "rejthar@stanislavrejthar.com",
    "issues": "https://github.com/WorkOfStan/seablast-i18n/issues",
    "source": "https://github.com/WorkOfStan/seablast-i18n"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Seablast\\I18n\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seablast\\I18n\\Tests\\": [
        "tests/"
      ]
    }
  }
}
